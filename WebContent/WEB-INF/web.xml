<?xml version="1.0" encoding="ISO-8859-1"?>
 <web-app>


    <servlet>
        <servlet-name>LoginUser</servlet-name>
        <servlet-class>servlet.LoginUser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginUser</servlet-name>
        <url-pattern>/user/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LogoutUser</servlet-name>
        <servlet-class>servlet.LogoutUser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogoutUser</servlet-name>
        <url-pattern>/user/logout</url-pattern>
    </servlet-mapping>

	<servlet>
        <servlet-name>RegisterUser</servlet-name>
        <servlet-class>servlet.RegisterUser</servlet-class>
	</servlet>
	<servlet-mapping>
        <servlet-name>RegisterUser</servlet-name>
        <url-pattern>/user/register</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AddPwitt</servlet-name>
        <servlet-class>servlet.AddPwitt</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddPwitt</servlet-name>
        <url-pattern>/pwitt/add</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>FindPwitt</servlet-name>
        <servlet-class>servlet.FindPwitt</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FindPwitt</servlet-name>
        <url-pattern>/pwitt/find</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AddFriend</servlet-name>
        <servlet-class>servlet.AddFriend</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddFriend</servlet-name>
        <url-pattern>/friend/add</url-pattern>
    </servlet-mapping>

	<servlet>
        <servlet-name>AddFollower</servlet-name>
        <servlet-class>servlet.AddFollower</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddFollower</servlet-name>
        <url-pattern>/follower/add</url-pattern>
    </servlet-mapping>
      
    <servlet>
        <servlet-name>RemoveFriend</servlet-name>
        <servlet-class>servlet.RemoveFriend</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RemoveFriend</servlet-name>
        <url-pattern>/friend/remove</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>MapReduce</servlet-name>
        <servlet-class>servlet.MapReduce</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MapReduce</servlet-name>
        <url-pattern>/server/mapreduce</url-pattern>
    </servlet-mapping>
    
 </web-app>
